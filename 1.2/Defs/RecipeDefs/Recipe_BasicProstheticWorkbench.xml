<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <RecipeDef ParentName="RecipeBasicProsthesis">
    <defName>CreateSpoonHand</defName>
    <label>Craft spoon hand</label>
    <description>Crafts a spoon hand from metal.</description>
    <jobString>Crafting prosthesis.</jobString>
    <soundWorking>Recipe_MakeWoodPlanks_Hand</soundWorking>
    <products>
      <SpoonHand>1</SpoonHand>
    </products>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
      <categories>
        <li>Textiles</li>
      </categories>
    </fixedIngredientFilter>
  </RecipeDef>
